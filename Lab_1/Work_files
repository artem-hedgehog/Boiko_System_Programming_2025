@artem-hedgehog ➜ /workspaces/Boiko_System_Programming_2025/Work $ for file in ФИО/T*; do
    if [ -f "$file" ]; then
        echo "=== Содержимое файла: $file ===" >> 'Лабораторная работа №1!/Result_3' 
        cat "$file" >> 'Лабораторная работа №1!/Result_3' 
        echo -e "\n" >> 'Лабораторная работа №1!/Result_3' 
    elif [ -d "$file" ]; then
        echo "=== Содержимое папки: $file ===" >> 'Лабораторная работа №1!/Result_3' 
        ls -la "$file" >> 'Лабораторная работа №1!/Result_3' 
        echo -e "\n" >> 'Лабораторная работа №1!/Result_3' 
    fi
done
